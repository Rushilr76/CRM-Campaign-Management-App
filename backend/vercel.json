{
    "version": 2,
    "name" : "Mini-CRM",
    "builds": [
      { "src": "./src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
      "src": "/api/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
    ]
  }
